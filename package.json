{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "coverage": "vitest --coverage",
    "format:icons-json": "eslint packages/*/icons.json --fix --no-ignore",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@iconify/tools": "^4.1.2",
    "@ntnyq/eslint-config": "^3.12.3",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "unocss": "^66.0.0",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml,svg}": "eslint --fix"
  }
}
