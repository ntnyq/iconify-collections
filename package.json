{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "format:icons-json": "eslint packages/*/icons.json --fix --no-ignore",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@iconify/tools": "^4.1.4",
    "@ntnyq/eslint-config": "^5.8.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unocss": "^66.5.6",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,svg,yml,yaml,json}": "eslint --fix"
  }
}
