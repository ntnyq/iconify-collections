{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate lint:fix",
    "coverage": "vitest --coverage",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky install",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@iconify/tools": "^3.0.7",
    "@ntnyq/eslint-config": "^1.19.0",
    "@ntnyq/prettier-config": "^1.19.0",
    "@types/node": "^20.10.1",
    "@vitest/coverage-v8": "^1.0.0-beta.6",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "tsx": "^4.6.1",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "vite": "^5.0.4",
    "vitest": "^1.0.0-beta.6"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
