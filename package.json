{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "coverage": "vitest --coverage",
    "format:icons-json": "eslint packages/*/icons.json --fix",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@iconify/tools": "^4.0.7",
    "@ntnyq/eslint-config": "^2.8.2",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "unocss": "^0.63.4",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
