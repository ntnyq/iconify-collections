{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "coverage": "vitest --coverage",
    "format:icons-json": "eslint packages/*/icons.json --fix --no-ignore",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@iconify/tools": "^4.0.7",
    "@ntnyq/eslint-config": "^3.9.1",
    "@ntnyq/prettier-config": "^1.22.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unocss": "^0.65.1",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
