{
  "name": "iconify-collections",
  "private": true,
  "packageManager": "pnpm@8.6.4",
  "license": "MIT",
  "scripts": {
    "coverage": "vitest --coverage",
    "generate": "tsx scripts/generateCollections.ts && eslint . --fix",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky install",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@iconify/tools": "^3.0.2",
    "@ntnyq/eslint-config": "^1.9.0",
    "@ntnyq/prettier-config": "^1.9.0",
    "@types/node": "^20.3.1",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
