{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "coverage": "vitest --coverage",
    "format:icons-json": "eslint packages/*/icons.json --fix",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky install",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@iconify/tools": "^4.0.4",
    "@ntnyq/eslint-config": "^1.21.2",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^20.12.8",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "3.2.5",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
