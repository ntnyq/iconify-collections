{
  "name": "iconify-collections",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "license": "MIT",
  "scripts": {
    "build": "run-s generate format:icons-json",
    "coverage": "vitest --coverage",
    "format:icons-json": "eslint packages/*/icons.json --fix",
    "generate": "tsx scripts/generateCollections.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky install",
    "publish": "pnpm generate && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@iconify/tools": "^4.0.0",
    "@ntnyq/eslint-config": "^1.20.2",
    "@ntnyq/prettier-config": "^1.20.2",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
